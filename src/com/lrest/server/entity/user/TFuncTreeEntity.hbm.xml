<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lrest.server.entity.user.TFuncTreeEntity" table="t_func_tree" schema="test">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>
        <property name="pid">
            <column name="PID" sql-type="int(11)"/>
        </property>
        <property name="funcTitle">
            <column name="FUNC_TITLE" sql-type="varchar(200)" length="200"/>
        </property>
        <property name="platform">
            <column name="PLATFORM" sql-type="int(11)"/>
        </property>
        <property name="funcCode">
            <column name="FUNC_CODE" sql-type="varchar(200)" length="200"/>
        </property>
        <property name="seq">
            <column name="SEQ" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>